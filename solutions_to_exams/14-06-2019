   "source": [
    "img = mpimg.imread('esame1_ex1.jpg')\n",
    "\n",
    "fig, axes = plt.subplots(1, 1, figsize = (14, 8))\n",
    "\n",
    "plt.xticks([])\n",
    "plt.yticks([])\n",
    "\n",
    "for i in ['top', 'left', 'bottom', 'right']:\n",
    "    plt.gca().spines[i].set_visible(False)\n",
    "\n",
    "plt.imshow(img)\n",
